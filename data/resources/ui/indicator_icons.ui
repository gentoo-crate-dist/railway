<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.24"/>
  <requires lib="libadwaita" version="1.0"/>

  <template class="DBIndicatorIcons" parent="GtkBox">
    <property name="accessible-role">group</property>
    <accessibility>
      <relation name="described-by">img_load_factor</relation>
      <relation name="described-by">img_late_factor</relation>
      <relation name="described-by">img_change_platform</relation>
      <relation name="described-by">img_unreachable</relation>
      <relation name="described-by">img_cancelled</relation>
    </accessibility>
    <property name="orientation">horizontal</property>
    <property name="spacing">2</property>
    <child>
      <object class="GtkImage" id="img_load_factor">
        <property name="visible">false</property>
      </object>
    </child>
    <child>
      <object class="GtkImage" id="img_late_factor">
        <property name="visible">false</property>
      </object>
    </child>
    <child>
      <object class="GtkImage" id="img_change_platform">
        <property name="visible">false</property>
      </object>
    </child>
    <child>
      <object class="GtkImage" id="img_unreachable">
        <property name="visible">false</property>
      </object>
    </child>
    <child>
      <object class="GtkImage" id="img_cancelled">
        <property name="visible">false</property>
      </object>
    </child>
  </template>
</interface>
